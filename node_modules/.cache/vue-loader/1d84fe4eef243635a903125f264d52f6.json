ue=\"1\">\n                  <v-row>\n                    <v-col cols=\"12\" md=\"8\">\n                      <v-card-text class=\"mt-12\">\n                        <h1 class=\"text-center display-2 yellow--text text--darken-1\">Sign in</h1>\n                        <div class=\"text-center mt-4\">\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-facebook-f</v-icon>\n                          </v-btn>\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-google-plus-g</v-icon>\n                          </v-btn>\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-linkedin-in</v-icon>\n                          </v-btn>\n                          <h4 class=\"text-center mt-4\">Ensure your email for registration</h4>\n                        </div>\n                          <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                            <v-text-field id=\"email\" :rules=\"emailRules\" label=\"Email\" name=\"Email\" prepend-icon=\"email\" type=\"text\" color=\"teal-accent-3\"></v-text-field>\n                            <v-text-field id=\"password\" :rules=\"emailPassword\" label=\"Password\" name=\"Password\" prepend-icon=\"lock\" type=\"password\" color=\"teal-accent-3\"></v-text-field>\n                          </v-form>\n                          <v-dialog v-model=\"dialog\" width=\"500\">\n                            <template v-slot:activator=\"{ on, attrs }\">\n                              <v-btn class=\"text-decoration-underline text-center mt-3\" v-bind=\"attrs\" v-on=\"on\" text>\n                                Forgot your password?\n                              </v-btn>\n                            </template>\n                      \n                            <v-card>\n                              <v-card-title class=\"headline yellow darken-1 text-end\">\n                                Forgot your password?\n                              </v-card-title>\n                      \n                              <v-card-text>\n                                <p>Dont worry. Resetting your password is easy, just tell us the email address you registered with.</p>\n                                <v-text-field v-model=\"email\" :rules=\"emailRules\" label=\"E-mail\" required></v-text-field>\n                              </v-card-text>\n                      \n                              <v-divider></v-divider>\n                      \n                              <v-card-actions>\n                                <v-btn color=\"grey lighten-1\" class=\"mr-4\" @click=\"dialog = false\">\n                                  Cancel\n                                </v-btn>\n                                <v-spacer></v-spacer>\n                                <v-btn color=\"yellow darken-1\" class=\"mr-4\" @click=\"dialog = false\">\n                                  Send\n                                </v-btn>\n                              </v-card-actions>\n                            </v-card>\n                          </v-dialog>\n                      </v-card-text>\n                      <div class=\"text-center mt-3\">\n                        <v-btn rounded :disabled=\"!valid\" x-large class=\"yellow darken-1 text-center\" @click=\"validate\" to=\"/main\">SIGN IN</v-btn>\n                      </div>\n                    </v-col>\n                    <v-col cols=\"12\" md=\"4\" class=\"yellow darken-1\">\n                      <v-card-text class=\"black--text mt-12\">\n                        <h1 class=\"text-center display-1\">Hello, Friends!</h1>\n                        <h4 class=\"text-center\">Enter your personnel details and join us!</h4>\n                      </v-card-text>\n                      <div class=\"text-center\">\n                        <v-btn rounded outlined=\"\" @click=\"step++\">SIGN UP</v-btn>\n                      </div>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n                <v-window-item :value=\"2\">\n                  <v-row class=\"fill-height\">\n                    <v-col cols=\"12\" md=\"4\" class=\"yellow darken-1\">\n                    <v-card-text class=\"black--text mt-12\">\n                      <h1 class=\"text-center display-1\">Welcome Back!</h1>\n                      <h4 class=\"text-center\">To keep connected with us please login with your personnel info</h4>\n                    </v-card-text>\n                    <div class=\"text-center\">\n                      <v-btn rounded outlined @click=\"step--\">SIGN IN</v-btn>\n                    </div>\n                    </v-col>\n                    <v-col cols=\"12\" md=\"8\">\n                      <v-card-text class=\"mt-12\">\n                       <h1 class=\"text-center display-2 yellow--text text--darken-1\">Create Account</h1>\n                       <div class=\"text-center mt-4\">\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-facebook-f</v-icon>\n                          </v-btn>\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-google-plus-g</v-icon>\n                          </v-btn>\n                          <v-btn class=\"mx-2\" fab color=\"black\" outlined>\n                            <v-icon>fab fa-linkedin-in</v-icon>\n                          </v-btn>\n                       </div>\n                       <h4 class=\"text-center mt-4\">Ensure your email for registration</h4>\n                       <v-form ref=\"form2\" v-model=\"valid2\" lazy-validation>\n                            <v-text-field id=\"name\" label=\"Name\" name=\"Name\" :rules=\"nameRules\" prepend-icon=\"person\" type=\"text\" color=\"teal-accent-3\"></v-text-field>\n                            <v-text-field id=\"email\" :rules=\"emailRules\" label=\"Email\" name=\"Email\" prepend-icon=\"email\" type=\"text\" color=\"teal-accent-3\"></v-text-field>\n                            <v-text-field id=\"password\" v-model=\"password\" :rules=\"passwordRules\" label=\"Password\" name=\"Password\" prepend-icon=\"lock\" type=\"password\" color=\"teal-accent-3\"></v-text-field>\n                            <v-text-field label=\"Confirm Password\" v-model=\"password2\" :rules=\"cPasswordRules\" name=\"CPassword\" prepend-icon=\"lock\" type=\"password\" color=\"teal-accent-3\"></v-text-field>\n                          </v-form>\n                          <div class=\"text-center mt-5\">\n                            <v-btn rounded :disabled=\"!valid2\" x-large color=\"yellow darken-1 text-center\" class=\"mr-4\" @click=\"validate2\" to=\"/main\">SIGN UP</v-btn>\n                          </div>\n                      </v-card-text>\n                    </v-col>\n                  </v-row>\n                </v-window-item>\n              </v-window>\n            </v-card>\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-content>\n  </v-app>\n</template>\n\n<script>\n// @ is an alias to /src\nexport default {\n  data: () => ({\n    step: 1 ,\n    valid: true,\n    valid2: true,\n    name: '',\n    password: '',\n    password2: '',\n    dialog: false,\n    dialog2: false,\n    show1: false,\n    email: '',\n    passwordRules: [\n        value => !!value || 'Required.',\n        v => v.length >= 8 || 'Min 8 characters',\n    ],\n    nameRules: [\n      v => !!v || 'Name is required',\n      v => (v && v.length < 15) || 'Name must be less than 15 characters',\n    ],\n    emailRules: [\n      v => !!v || 'E-mail is required',\n      v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n    ],\n    cPasswordRules: [\n      v => !!v || 'Confirmation is Required',\n      v => (v && v === this.password) || 'It must be the same as the password above.',\n    ]\n  }),\n  props: {\n    source: String\n  },\n  methods: {\n    validate () {\n      this.$refs.form.validate()\n    },\n    validate2 () {\n      this.$refs.form2.validate()\n    },\n    reset () {\n      this.$refs.form.reset()\n    },\n  }\n}\n</script>\n\n<style scoped>\n.dark {\n  color: rgb(34, 40, 49)\n}\n</style>"]}]}